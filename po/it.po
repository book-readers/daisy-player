msgstr "Impossibile leggere % s"

#: src/daisy-player.c:337
msgid "'h' for help -"
msgstr "h per aiuto"

#: src/daisy-player.c:341
#, c-format
msgid "%d pages"
msgstr "%d pagine"

#: src/daisy-player.c:346
