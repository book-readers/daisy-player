# 1
msgid " %d pages "
msgstr " %d sider"

# 2
msgid " (%2d of %2d)"
msgstr " (%2d of %2d)"

# 3
msgid " level: %d of %d "
msgstr " nivå: %d of %d "

# 4
msgid " total length: %02d:%02d:%02d "
msgstr " total lengde: %02d:%02d:%02d "   

# 5
msgid "'h' for help -"
msgstr "'h' for hjelp -"

# 6
msgid "A parser to play Daisy CD's with Linux"
msgstr "Et program for å spille Daisy CD's med Linux"

# 7
msgid "Cannot find a daisy structure in %s"
msgstr "Kan ikke finne en daisy struktur i %s"

# 8
msgid "Cannot find a daisy structure"
msgstr "Kan ikke finne en daisy struktur"

# 9
msgid "Corrupt daisy structure %s\n"
msgstr "Korrupt daisy struktur %s\n"

# 10
msgid "Corrupt daisy structure!"
msgstr "Korrupt daisy struktur!"

# 11
msgid "D               - decrease playing speed\n"
msgstr "D               - reduserer spille tempo\n"

# 12
msgid "Daisy-player - Version %s - (C)2013 J. Lemmens"
msgstr "Daisy-player - Versjon %s - (C)2013 J. Lemmens"

# 13
msgid "Daisy-player - Version %s\n"
msgstr "Daisy-player - Versjon %s\n"

# 14
msgid "Daisy-player can't handle this DTB."
msgstr "Daisy-player kan ikke håndtere DTB."

# 15
msgid "Go to page number: "
msgstr "Gå til side nummer:"

# 16
msgid "L               - switch to previous level\n"
msgstr "L               - gå til forrige nivål\n"

# 17
msgid "Maybe a read-error occurred!"
msgstr "Kanskje et skrivefeil skjedde!"

# 18
msgid "N               - search backwards\n"
msgstr "N               - søk omvendt\n"

# 19
msgid "No such directory: %s\n"
msgstr "Ikke en slik mappe: %s\n"

# 20
msgid "Please wait... -------------------------"
msgstr "Vennligst vent.. -------------------------"

# 21
msgid "Please wait..."
msgstr "Vennligst vent..."

# 22
msgid "Press 'h' for help "
msgstr "Trykk 'h' for hjelp "

# 23
msgid "Press any key for next page..."
msgstr "Trykk en tilfeldig knapp for neste side..."

# 24
msgid "Reading %s... "
msgstr "Leser %s... "

# 25
msgid "Reading daisy structure..."
msgstr "Leser daisy struktur..."

# 26
msgid "Something is wrong with %s\n"
msgstr "Noe går galt med %s\n"

# 27
msgid "Something is wrong with the %s file\n"
msgstr "Noe går galt med den %s filen\n"

# 28
msgid "This is not possible!"
msgstr "Dette er ikke mulig!"

# 29
msgid "U               - increase playing speed\n"
msgstr "U               - øker spille tempo\n"

# 30
msgid "What do you search? "
msgstr "Hva søker du?"

# 31
msgid "\n%s: is not a sound device.\n"
msgstr "\n%s: er ikke et lydkart.\n"

# 32
msgid "\n/               - search for a label\n"
msgstr "\n/               - søker en etikett\n"

# 33
msgid "\nCannot read %s\n"
msgstr "\nKan ikke lese %s\n"

# 34
msgid "\nCannot read /proc/mounts."
msgstr "\nKan ikke lese /proc/mounts."

# 35
msgid "\nCannot use udisks command."
msgstr "\nKan ikke bruke udisks kommando."

# 36
msgid "\nNo daisy-cd in drive."
msgstr "\nIngen daisy-cd i spillern."

# 37
msgid "\nPress any key to leave help..."
msgstr "\nTrykk en tilfeldig knapp for å forlate hjelp..."

# 38
msgid "\nThese commands are available in this version:\n"
msgstr "\nDisse kommandoer er tilgjengelig i denne versjon:\n"

# 39
msgid "\nUsage: %s [-l language] [directory with a Daisy-structure]\n"
msgstr "\nForbruk: %s [-l språk] [mappe med Daisy-struktur]\n"

# 40
msgid "clip_end (%f) is smaller than clip_begin (%f).\n"
msgstr "clip_end (%f) er mindre enn clip_begin (%f).\n"

# 41
msgid "cursor down     - move cursor to the next item\n"
msgstr "markør ned           - flytt markør til neste element\n"

# 42
msgid "cursor left     - skip to previous phrase\n"
msgstr "markør til venstre   - hopp over til forrige settning\n"

# 43
msgid "cursor right    - skip to next phrase\n"
msgstr "markør til høyre     - hopp over til neste settning\n"

# 44
msgid "cursor up       - move cursor to the previous item\n"
msgstr "markør opp           - flytt markør til forrige element\n"

# 45
msgid "d               - store current item to disk\n"
msgstr "d               - oppbevarer gjeldende element til disk\n"

# 46
msgid "enter           - start playing\n"
msgstr "enter                - begynner å spille\n"

# 47
msgid "f               - find the currently playing item and place the cursor there\n"
msgstr "f               - finner den spillende elementen og plasserer markøren der\n"

# 48
msgid "g               - go to page number (if any)\n"
msgstr "g               - går til sidenummer (hvis det finnes)\n"

# 49
msgid "h or ?          - give this help\n"
msgstr "h or ?          - viser hjelp\n"

# 50
msgid "home            - play on normal speed\n"
msgstr "home                 - spiller på normal tempo\n"

# 51
msgid "j               - just play current item\n"
msgstr "j               - spiller gjeldende element\n"

# 52
msgid "l               - switch to next level\n"
msgstr "l               - går til neste nivå\n"

# 53
msgid "n               - search forwards\n"
msgstr "n               - søker framover\n"

# 54
msgid "n length: %d\n"
msgstr "n lengde: %d\n"

# 55
msgid "o               - select next output sound device\n"
msgstr "o               - selekterer neste lydkart\n"

# 56
msgid "p               - place a bookmark\n"
msgstr "p               - lager en bokmerke\n"

# 57
msgid "page-down       - view next page\n"
msgstr "page-down            - viser neste side\n"

# 58
msgid "page-up         - view previous page\n"
msgstr "page-up              - viser forrige side\n"

# 59
msgid "q               - quit daisy-player and place a bookmark\n"
msgstr "q               - avslutter daisy-player og lager en bokmerke\n"

# 60
msgid "s               - stop playing\n"
msgstr "s               - stopper å spille\n"

# 61
msgid "s length: %d\n"
msgstr "s lengde: %d\n"

# 62
msgid "space           - pause/resume playing\n"
msgstr "space                - pauserer/gjennopptar spilling\n"
