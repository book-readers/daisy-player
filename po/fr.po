msgstr "L           - remonter au niveau précédent\n"

# 53
#: src/daisy-player.c:788
msgid "n               - search forwards\n"
msgstr "n           - recherche sur la suite\n"

# 18
#: src/daisy-player.c:789
msgid "N               - search backwards\n"
msgstr "N           - recherche inverse\n"

# 55
