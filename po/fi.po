msgid "Select an Audio_book:"
msgstr ""

#: src/daisy-player.c:2167
#, c-format
msgid "A parser to play Daisy CD's with Linux"
msgstr "Jsennin Daisy-CD-levyjen toistamiseen Linuxissa"

#: src/daisy-player.c:2169
#, c-format
msgid "Scanning for a Daisy CD..."
msgstr "Etsitn Daisy-CD:t..."

#: src/daisy-player.c:2270 src/daisy-player.c:2408
msgid "No DAISY-CD or Audio-cd found"
msgstr "DAISY- tai ni-CD:t ei lytynyt"

#: src/daisy-player.c:2326 src/daisy-player.c:2337 src/daisy-player.c:2365
msgid "No Daisy CD in drive."
msgstr "Asemassa ei ole Daisy-CD:t."

#: src/daisy-player.c:2380
msgid "Found an Audio-CD."
msgstr "Lytyi ni-CD."

#: src/daisy-player.c:2385
#, c-format
msgid "Get titles from freedb.freedb.org..."
msgstr "Hae kappaleiden nimet freedb.freedb.org:ista..."

#: src/daisy-player.c:2587
msgid "Press 'h' for help"
msgstr "Saat ohjeita painamalla 'h'"

#: src/ncx.c:262
msgid "No items found. Try option \"-O\"."
msgstr "Kohteita ei lytynyt. Kokeile valitsinta \"-O\"."

