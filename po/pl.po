#: src/daisy-player.c:422
msgid "'h' for help -"
msgstr "'h' o pomoc - "

#: src/daisy-player.c:424
#, c-format
msgid " %d pages "
msgstr " %d strony "

#: src/daisy-player.c:425
#, c-format
